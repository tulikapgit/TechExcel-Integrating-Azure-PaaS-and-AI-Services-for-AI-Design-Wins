{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to .NET Functions (Windows)",
      "type": "coreclr",
      "request": "attach",
      "preLaunchTask": "host start",
      "processId": "${command:azureFunctions.pickProcess}",
      "pipeTransport": {
        "pipeCwd": "${workspaceFolder}",
        "pipeProgram": "C:\\Windows\\System32\\bash.exe",
        "pipeArgs": ["-c"],
        "debuggerPath": "/home/adam/.vscode/extensions/ms-dotnettools.csharp-1.23.0/.debugger/vsdbg",
        "quoteArgs": true
      }
    },
  {
